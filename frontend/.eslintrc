{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "@jetbrains",
    "@jetbrains/eslint-config/node",
    "@jetbrains/eslint-config/es6",
    "@jetbrains/eslint-config/react",
    "prettier",
    "prettier/react"
  ],
  "settings": {
    "react": {"version": "detect"}
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "semi": "off",
    "no-unused-expressions": "off",
    "import/no-commonjs": "off",
    "linebreak-style": "off",
    "prettier/prettier": "warn",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true,
        "optionalDependencies": false,
        "peerDependencies": false
      }
    ],
    "import/no-cycle": "error",
    "curly": ["warn", "all"],
    "camelcase": ["warn", {
      "properties": "never"
    }],
    "react/prop-types": "off"
  },
  "globals": {
    "cssVariables": true,
    "window": true,
    "document": true
  },
  "overrides": [
    {
      "files": [
        "jest.setup.js",
        "*.test.js",
        "*-test.js",
        "a11y-audit.js"
      ],
      "globals": {
        "React": false,
        "mount": false,
        "shallow": false,
        "render": false
      },
      "env": {
        "jest": true,
        "browser": true
      }
    }
  ]
}
